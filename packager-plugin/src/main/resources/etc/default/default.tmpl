# ${generatedText}

VERSION=${version}
MAIN=${mainClass}
CLASSPATH=${classpath}
LOGSETTING="-lx ${logConfigPath}"
CONFIG=${configPath}
HOSTNAME=$(hostname)
# enable jmx by giving free port number and uncomment following two lines.
# JMX_PORT=9000
# JMX="-Djavax.management.builder.initial=org.vesalainen.jmx.SimpleMBeanServerBuilder -Dorg.vesalainen.jmxremote.port=$JMX_PORT"
DAEMON=/usr/bin/java
LDPATH=-Djava.library.path=/opt/org.vesalainen/lib
DAEMON_ARGS="-server -classpath $CLASSPATH $JMX $LDPATH $MAIN $LOGSETTING $CONFIG"


